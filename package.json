{
  "name": "@hubstairs/display-controller",
  "version": "0.0.0",
  "description": "Interact with and control an embedded Hubstairs display.",
  "license": "MIT",
  "author": "Guillaume HERTAULT <gh@hubstairs.com>",
  "scripts": {
    "start": "npm run build -- --watch",
    "build": "BABEL_ENV=production node rollup.js",
    "lint": "eslint src",
    "test": "nyc --cache --reporter=text --reporter=html ava",
    "prettier:fix": "prettier --write '**/*.{js,jsx,json,md,css,scss,sh,yml,yaml,html}'"
  },
  "main": "dist/player.js",
  "module": "dist/player.es.js",
  "jsnext:main": "dist/player.es.js",
  "directories": {
    "lib": "src/"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "hubstairs",
    "display",
    "api",
    "iframe",
    "embed",
    "javascript",
    "postMessage"
  ],
  "dependencies": {
    "native-promise-only": "0.8.1",
    "weakmap-polyfill": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.3.3",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "chalk": "^3.0.0",
    "chokidar": "^3.0.0",
    "chokidar-cli": "^2.0.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-ava": "^6.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "esm": "^3.2.5",
    "husky": "^4.2.1",
    "jquery": "^3.3.1",
    "jsdom": "^15.2.1",
    "lint-staged": "^10.0.3",
    "maxmin": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.0.0",
    "prettier": "^1.19.1",
    "rollup": "^1.2.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "sinon": "^7.2.4",
    "uglify-js": "^3.4.9",
    "xhr2": "0.2.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "shippedProposals": true
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/transform-runtime"
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "require": [
      "esm",
      "@babel/register",
      "./test/helpers/browser-env.js"
    ],
    "sources": [
      "!dist/**/*"
    ]
  },
  "nyc": {
    "require": [
      "esm",
      "@babel/register"
    ],
    "exclude": [
      "node_modules",
      "src/lib/compatibility-check.js",
      "test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,json,md,css,scss,yml,yaml,html}": [
      "prettier -l"
    ]
  }
}
